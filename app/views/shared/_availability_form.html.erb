<%= form_for(@availability, html: { multipart: true }) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="field">
    <%= f.label :meeeting_time %>
    <%= f.text_field :meeeting_time, placeholder: "Time of your meeting" %>
  </div>
  <div class="field">
    <%= f.label :meeeting_location %>
    <%= f.text_field :meeeting_location, placeholder: "Location of your meeting" %>
  </div>
  <%= f.submit "Post", class: "btn btn-primary" %>
<% end %>

<%= form_for(@course, html: { multipart: true }) do |f| %>
  <div class="field">
    <%= f.label :course_id %>
    <%= f.text_field :course_id, placeholder: "CSC 121" %>
  </div>
  <div class="field">
    <%= f.label :description %>
    <%= f.text_area :description, placeholder: "Course description" %>
  </div>
  <%= f.submit "Post", class: "btn btn-primary" %>
<% end %>
<%= link_to "Done", "/profile" %>
